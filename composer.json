{
    "name": "ivory/google-map-bundle",
    "type": "symfony-bundle",
    "description": "Provides a google map integration for Symfony",
    "keywords": [ "google", "map" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Breu",
            "email": "samuel.breu@bresam.ch",
            "role": "Developer/Maintainer"
        },
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com",
            "role": "Creator/Inactive"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "symfony/framework-bundle": "^3.0 || ^4.0 || ^5.0",
        "ivory/google-map": "^3.0"
    },
    "require-dev": {
        "ivory/serializer-bundle": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "php-http/cache-plugin": "^1.4",
        "php-http/guzzle6-adapter": "^1.0",
        "phpunit/phpunit": "^6.5",
        "phpunit/phpunit-selenium": "^4.1.0",
        "symfony/cache": "^3.2 || ^4.0",
        "symfony/form": "^2.7 || ^3.0 || ^4.0",
        "symfony/phpunit-bridge": "^2.7 || ^3.0 || ^4.0",
        "symfony/templating": "^2.7 || ^3.0 || ^4.0",
        "symfony/yaml": "^2.7 || ^3.0 || ^4.0",
        "twig/twig": "^3.0"
    },
    "autoload": {
        "psr-4": { "Ivory\\GoogleMapBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Ivory\\Tests\\GoogleMap\\": "vendor/ivory/google-map/tests" }
    },
    "replace": {
        "egeloen/google-map-bundle": "^3.0.1"
    }
}
